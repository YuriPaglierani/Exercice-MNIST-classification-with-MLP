   ],
   "source": [
    "cm = confusion_matrix(Y_test, Y_pred) \n",
    "plt.figure(figsize=(9, 9))\n",
    "sns.heatmap(cm, annot=True, cmap='Blues_r', linewidths=.5, square=True)\n",
    "plt.ylabel('Correct classes')\n",
    "plt.xlabel('Predicted classes')\n",
    "plt.show()"
   ]
  },
  {
